{
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.6.0"
        }
    },
    "routes": [
        { "src": "/static/(.*)", "dest": "/api/static/globals.css", "headers": { "cache-control": "max-age=31536000, immutable" }},
        { "src": "/(.*)", "dest": "/api/index.php" }
    ]
}
